			"baseColor2",
			"baseColor3",
			"baseColor4",
			"baseColor5",
			"baseColor6",
			"baseColor7",
			"baseColor8",
			"baseColor9",
			"baseColor10"
		],
		"ansi": [
			"ansiColor1",
			"baseColor9", //same as baseColor9
			"baseColor5", //same as baseColor5
			"baseColor10", //same as baseColor10
			"baseColor8", //same as baseColor8
			"baseColor7", //same as baseColor7
			"baseColor4", //same as baseColor4
			"baseColor1", //same as baseColor1
			"baseColor3", //same as baseColor3
			"ansiColor10",
			"ansiColor11",
			"ansiColor12",
			"ansiColor13",
			"ansiColor14",
			"ansiColor15",
			"ansiColor16"
		],
		"brightOther": ["brightColor0", "brightColor1"],
		"other": [
			"baseColor2+75",
			"ansiColor16+1A",
			"ansiColor16", //same as ansiColo16
			"baseColor2+70",
			"otherColor5",
			"otherColor6",
			"ansiColor1", //same as ansiColor1
			"otherColor8"
		]
	},
